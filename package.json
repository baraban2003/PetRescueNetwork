{
  "name": "PetRescueNetwork",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "homepage": "https://baraban2003.github.io/PetRescueNetwork",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "format": "prettier --write .",
    "lint": "eslint .",
    "type-check": "tsc",
    "prepare": "husky install",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.7",
    "@mui/material": "^5.15.7",
    "@mui/x-date-pickers": "^6.19.3",
    "@reduxjs/toolkit": "^1.8.1",
    "@types/lodash": "^4.14.202",
    "@types/react-bootstrap": "^0.32.35",
    "axios": "^1.6.7",
    "bootstrap": "^5.3.2",
    "classnames": "^2.3.2",
    "gh-pages": "^6.0.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.0",
    "react-datepicker": "^6.1.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-loader-spinner": "^6.1.6",
    "react-material-ui-form-validator": "^3.0.1",
    "react-phone-input-2": "^2.15.1",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.20.0",
    "react-scripts": "5.0.1",
    "react-toastify": "^10.0.4",
    "react-transition-group": "^4.4.5",
    "redux-persist": "^6.0.0",
    "vite-plugin-svgr": "^4.2.0"
  },
  "overrides": {
    "typescript": "^5.0.2"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.2.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/react-material-ui-form-validator": "^2.1.4",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.0",
    "jsdom": "^21.1.0",
    "lint-staged": "^14.0.1",
    "prettier": "^2.7.1",
    "prettier-config-nick": "^1.0.2",
    "typescript": "^5.0.2",
    "vite": "^4.0.0",
    "vitest": "^0.30.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "react/jsx-no-target-blank": "off"
    }
  },
  "prettier": "prettier-config-nick",
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "npx eslint --fix"
    ]
  }
}
